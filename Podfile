# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'Chat' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Chat
# Resource
pod 'R.swift'
# Firebase
pod 'Firebase/Core'
pod 'Firebase/Auth'
pod 'Firebase/Database'
# Facebook login
pod 'FBSDKLoginKit'
# GoogleSignIn
pod 'GoogleSignIn', '< 5.0.0'
end
